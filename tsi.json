    "components": {
        "securitySchemes": {
            "TsiAuth": {
                "description": "Authentication for TSI RESTful API",
                "type": "apiKey",
                "in": "header",
                "name": "X-API-KEY"
            },
            "UserAuth": {
                "description": "Auth yang mnggunakan login dengan username dan password",
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        },
        "parameters": {

        },
        "schemas": {
            "ProvinceList": {
                "type": "object",
                "properties": {
                    "kode_provinsi": {
                        "type": "number"
                    },
                    "nama_provinsi": {
                        "type": "string"
                    },
                    "versi": {
                        "type": "string"
                    },
                    "source": {
                        "type": "string"
                    }
                }
            },
            "UserList": {
                "type": "object",
                "properties": {
                    "user_id": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    },
                    "nik": {
                        "type": "integer"
                    },
                    "nama": {
                        "type": "object",
                        "properties": {
                            "nama_depan": {
                                "type": "string"
                            },
                            "nama_belakang": {
                                "type": "string"
                            }
                        }
                    },
                    "gelar": {
                        "type": "object",
                        "properties": {
                            "gelar_depan": {
                                "type": "string"
                            },
                            "gelar_belakang": {
                                "type": "string"
                            }
                        }
                    },
                    "kontak": {
                        "type": "object"
                    }
                }
            },
            "CreateUser": {
                "type": "object",
                "properties": {

                    "nama_depan": {
                        "type": "string"
                    },
                    "nama_belakang": {
                        "type": "string"
                    },
